# OpenRouter routing (server-side only)
# Set at least OPENROUTER_API_KEY_PRIMARY in Vercel Project -> Settings -> Environment Variables.

OPENROUTER_API_KEY_PRIMARY=replace_with_primary_key
OPENROUTER_MODEL_PRIMARY=z-ai/glm-4.5-air:free
OPENROUTER_LABEL_PRIMARY=GLM 4.5 Air (primary)

OPENROUTER_API_KEY_SECONDARY=replace_with_secondary_key
OPENROUTER_MODEL_SECONDARY=qwen/qwen3-vl-235b-a22b-thinking
OPENROUTER_LABEL_SECONDARY=Qwen 3 VL Thinking (backup)

OPENROUTER_API_KEY_TERTIARY=replace_with_tertiary_key
OPENROUTER_MODEL_TERTIARY=stepfun/step-3.5-flash:free
OPENROUTER_LABEL_TERTIARY=Step 3.5 Flash (backup)

# Optional metadata for OpenRouter headers
OPENROUTER_HTTP_REFERER=https://your-domain.example
OPENROUTER_APP_TITLE=AICENGHUB
