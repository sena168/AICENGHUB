# OpenRouter routing (server-side only)
# Set at least OPENROUTER_API_KEY_PRIMARY in Vercel Project -> Settings -> Environment Variables.

OPENROUTER_API_KEY_PRIMARY=replace_with_primary_key
OPENROUTER_MODEL_PRIMARY=z-ai/glm-4.5-air:free
OPENROUTER_LABEL_PRIMARY=GLM 4.5 Air (primary)

OPENROUTER_API_KEY_SECONDARY=replace_with_secondary_key
OPENROUTER_MODEL_SECONDARY=qwen/qwen3-vl-235b-a22b-thinking
OPENROUTER_LABEL_SECONDARY=Qwen 3 VL Thinking (backup)

OPENROUTER_API_KEY_TERTIARY=replace_with_tertiary_key
OPENROUTER_MODEL_TERTIARY=stepfun/step-3.5-flash:free
OPENROUTER_LABEL_TERTIARY=Step 3.5 Flash (backup)

# Optional metadata for OpenRouter headers
OPENROUTER_HTTP_REFERER=https://your-domain.example
OPENROUTER_APP_TITLE=AICENGHUB

# VPS tools server integration (server-side only)
TOOLS_BASE_URL=https://tools-server.example
TOOLS_API_KEY=replace_with_tools_api_key
TOOLS_TIMEOUT_MS=6000

# Neon database (server-side only)
NEON_DATABASE_URL=postgresql://username:password@host/database?sslmode=require

# Google OAuth admin authentication (for /admin.html)
GOOGLE_OAUTH_CLIENT_ID=replace_with_google_client_id
# Optional for future server-side OAuth code exchange flows
GOOGLE_OAUTH_CLIENT_SECRET=replace_with_google_client_secret
# Comma-separated allowlist for admin login
ADMIN_ALLOWED_EMAILS=senaprasena@gmail.com,ashmeeishwar@gmail.com,julehaautomata@gmail.com,suhuaiceng@gmail.com
# Secret used to sign admin auth session cookies
ADMIN_SESSION_SECRET=replace_with_random_session_secret
# Optional session lifetime in seconds (default: 43200 = 12 hours)
ADMIN_SESSION_TTL_SECONDS=43200

# Optional hardening: comma-separated browser origins allowed to call /api/juleha-chat
JULEHA_ALLOWED_ORIGINS=https://your-domain.example,https://www.your-domain.example

# Optional salt used to hash request IP/session for candidate audit trail storage.
# Keep server-side only.
JULEHA_AUDIT_SALT=replace_with_random_salt

# Optional: enable/disable server-side verification for links returned by Juleha.
# Use 1/true/on to enable, 0/false/off to disable.
JULEHA_VERIFY_LINKS=1

# Optional: enable/disable automatic candidate capture from Juleha responses.
# Use 1/true/on to enable, 0/false/off to disable.
JULEHA_CAPTURE_CANDIDATES=1
